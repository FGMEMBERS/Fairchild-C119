<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>tour</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>intvitres</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <path>Aircraft/Fairchild-C119/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -10.910 </x-m>
      <y-m>  -0.573 </y-m>
      <z-m>  -0.805 </z-m>
    </offsets>
  </model>

  <!-- Siège pilote -->
  <model>
    <path>Aircraft/Fairchild-C119/Models/Interior/seat/seat.xml</path>
    <offsets>
      <x-m> -10.762 </x-m>
      <y-m>  -0.588 </y-m>
      <z-m>  -0.851 </z-m>
    </offsets>
  </model>

  <!-- Siège copilote -->
  <model>
    <path>Aircraft/Fairchild-C119/Models/Interior/seat/seat.xml</path>
    <offsets>
      <x-m> -10.762 </x-m>
      <y-m>   0.588 </y-m>
      <z-m>  -0.851 </z-m>
    </offsets>
  </model>

